<div id="background_form">
  <div id="sub_course_navigation">
    <% @course_subtab = true %>
  </div>
  <div id="home_sub_form">
    <%# @course_subtab = true %>
   <br/> <h1 style="font-size: 16px;margin: 6px 30px;">Course Details</h1>
    <table width='50%' style="margin: 16px 38px;">
      <tr>
        <td>Name</td>
        <td><%= @course.name%></td>
      </tr>
      <tr>
        <td>Abstract</td>
        <td><%=@course.abstract%></td>
      </tr>
      <tr>
        <td>Prerequisites</td>
        <td><%=@course.prerequisites%></td>
      </tr>
      <tr>
        <td>Course Content</td>
        <td><%=@course.course_content%></td>
      </tr>
      <tr>
        <td>Language</td>
        <td><%=@course.language%></td>
      </tr>
      <tr>
        <td><%=link_to "edit", edit_course_path(@course) %></td>
        <td></td>
      </tr>
      
    </table>
  </div>
  <%= image_tag('banner shadow.png', :style=>"height: 11px;    margin: 35px -96px 10px;    width: 962px;") %>
  <!--<img alt="" src="images/banner shadow.png" style="height: 11px;    margin: 37px -96px 10px;    width: 962px;"/>-->
</div>
