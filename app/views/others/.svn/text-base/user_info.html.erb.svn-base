<% @profile_subtab_tab = true %>
<%  if current_user.user_type == 'student' %>
  <table>
    <% @students.each do |a| %>
      <u style="color:red">Personal Info</u>
      <td>Name:</td>
      <td><%= a.first_name  %></td>
      <tr>
        <td>Date Of Birth:</td>
        <td><%= a.date_of_birth %> </td>
      </tr>
      <tr>
        <td>Location:</td>
        <td><%= a.city %>, <%= a.state %>, <%= a.country %></td>
      </tr>
      <td style="color:red"><u>Contact Info</u></td>
      <tr>
        <td>Mobile No:</td>
        <td><%=  a.mobile %> </td>
      </tr>
      <tr>
        <td>LandLine No:</td>
        <td><%= a.land_line_no %> </td>
      </tr>
      <tr>
        <td>Email Id:</td>
        <td><%= current_user.email %> </td>
      </tr>
      <tr>
        <td style="color:red"><u> Education Details</u></td></tr>
      <tr>
        <td>Company:</td>
        <td><%= a.college_name %></td>
      </tr>
      <tr>
        <td>Designation:</td>
        <td><%=  a.designation %></td>
      </tr>
      <tr>
        <td>Qualification:</td>
        <td><%= a.qualification %></td>
      </tr>
      <tr>
        <td>City:</td>
        <td><%=  a.city %></td>
      </tr>
      <tr>
        <td>Degree:</td>
        <td><%= a.degree %></td>
      </tr>
      <tr>
        <td>Stream:</td>
        <td><%= a.stream %></td>
      </tr>
      <tr>
        <%if a.technical? ||a.non_technical? %>
          <td style="color:red"><u>Work</u></td>
        <% end %>
      </tr>
      <tr>
        <%if a.technical? ||a.non_technical? %>
          <td style="color:red"><u>Interest</u></td>
        <% end %>
      </tr>
      <tr>
        <%if a.technical? %>
          <td valign="top">Technical:</td>
          <td><%= a.technical %></td>
        <% else %>
        <% end %>

      </tr>
      <tr>
        <td></td>
      </tr> <tr>
        <%if a.non_technical? %>
          <td valign="top">Non-Technical:</td>
          <td><%= a.non_technical %></td>
        <% else %>
        <% end %>
      </tr>
    <% end %>
  </table>
<% elsif %>
  <table>
    <% @mentors.each do |a| %>
      <u style="color:green">Personal Info</u>
      <td>Name:</td>
      <td><%= a.first_name  %> </td>
      <tr>
        <td>Date Of Birth:</td>
        <td><%= a.date_of_birth %> </td>
      </tr>
      <tr>
        <td valign="top">Location:</td>
        <td><%= a.city %>, <%= a.state %>, <%= a.country %></td>
      </tr>
      <td style="color:green"><u>Contact Info</u></td>
      <tr>
        <td>Mobile No:</td>
        <td><%=  a.mobile %> </td>
      </tr>
      <tr>
        <td>LandLine No:</td>
        <td><%= a.lande_line_no %> </td>
      </tr>
      <tr>
        <td>Email Id:</td>
        <td><%= current_user.email %> </td>
      </tr>

      <tr>
        <td style="color:green"><u> Education Details</u></td></tr>
      <tr>
        <td>Company:</td>
        <td><%= a.college_name %></td>
      </tr>
      <tr>
        <td>Designation:</td>
        <td><%=  a.designation %></td>
      </tr>
      <tr>
        <td>Qualification:</td>
        <td><%= a.qualification %></td>
      </tr>
      <tr>
        <td>City:</td>
        <td><%=  a.city %></td>
      </tr>
      <tr>
        <td>Degree:</td>
        <td><%= a.degree %></td>
      </tr>
      <tr>
        <td>Stream:</td>
        <td><%= a.stream %></td>
      </tr>
      <tr>
        <% if a.technical? || a.non_technical? %>
          <td style="color:green"><u>Work</u></td>
        <% end %>
      <tr>
        <%  if a.technical? %>
          <td valign="top">Employer:</td>
          <td><%= a.technical %></td>
        <% else %>
        <% end %>
      </tr>
      <tr>
        <td></td>
      </tr>
      <tr>
        <% if a.designation? %>
          <td valign="top">Designation:</td>
          <td><%= a.designation %></td>
        <% else %>
        <% end %>
      </tr>
      <tr>
        <% if a.designation? %>
          <td valign="top">More Work:</td>
          <td><%= a.designation %></td>
        <% else %>
        <% end %>
      </tr>
    </tr>
    <tr>
      <% if a.technical? || a.non_technical? %>
        <td style="color:green"><u>Interest</u></td>
      <% end %>
    <tr>
      <%  if a.technical? %>
        <td valign="top">Technical:</td>
        <td><%= a.technical %></td>
      <% else %>
      <% end %>
    </tr>
    <tr>
      <td></td>
    </tr>
    <tr>
      <% if a.non_technical? %>
        <td valign="top">Non-Technical:</td>
        <td><%= a.non_technical %></td>
      <% else %>
      <% end %>
    </tr>
    </tr>
  <% end %>
  </table>
<% end %>

